       <script> 
    let url='https://shopping-cart-payment-api-100.herokuapp.com/discount/1000'
        console.log(url)
        https.get(url,function(response)
        {
            response.on("data",function(data){
            const weatherData = JSON.parse(data);
            console.log(weatherData.discount);
            discount=parseInt(weatherData.discount);
            console.log(discount);
            const input=document.getElementById("discount");
            input.innerHTML=discount;
             })
        })
             </script>
